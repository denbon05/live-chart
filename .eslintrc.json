{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": ["jest", "babel"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": [2, { "allow": ["__filename", "__dirname"] }],
        "import/extensions": 0
    }
}
